#include <iostream>
using namespace std;
int linearsearch(int arr[], int n, int key){
    for(int i=0;i<n;i++){
        if(arr[i]==key){
            return i;
        }
   
    }     return -1;
}
int main() {
	int n;
int arr[n];
	int key;
	cout<<"Enter no. of elements"<<endl;
	cin>>n;
	cout<<"Enter finding elements"<<endl;
	cin>>key;

	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
cout<<"index "<<linearsearch(arr,n,key);
}
